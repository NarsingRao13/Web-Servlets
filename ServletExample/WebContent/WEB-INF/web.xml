<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ServletExample</display-name>
  
  <context-param>
  		<param-name>DriverName</param-name>
		<param-value>com.mysql.jdbc.Driver</param-value>
		
  </context-param>
  
   <context-param>
   		<param-name>DatabaseLink</param-name>
	 	<param-value>jdbc:mysql://localhost:3306/narsing?characterEncoding=latin1</param-value>
   </context-param>
   
   <context-param>
   		<param-name>Username</param-name>
	 	<param-value>root</param-value>
   </context-param>

	 <context-param>
   		<param-name>Password</param-name>
	 	<param-value>root</param-value>
   </context-param>
   
   
   <servlet>
   		<servlet-name>Login</servlet-name>
   		<servlet-class>Login</servlet-class>
   		<init-param>
   			<param-name>logindata</param-name>
   			<param-value>Heyyyy</param-value>
   		</init-param>
   </servlet>
  
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>Body.html</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>